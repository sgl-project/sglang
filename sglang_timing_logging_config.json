{
  "version": 1,
  "formatters": {
    "default": {
      "format": "[%(asctime)s] %(message)s",
      "datefmt": "%Y-%m-%d %H:%M:%S"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "formatter": "default",
      "level": "INFO"
    },
    "timing_file": {
      "class": "logging.FileHandler",
      "filename": "sglang_request_timing.log",
      "formatter": "default",
      "level": "INFO",
      "mode": "w"
    }
  },
  "root": {
    "level": "INFO",
    "handlers": ["console"]
  },
  "loggers": {
    "sglang.srt.managers.schedule_batch": {
      "level": "INFO",
      "handlers": ["timing_file"],
      "propagate": false
    }
  }
}
