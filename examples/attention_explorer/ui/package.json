{
  "name": "latent-chat-explorer",
  "version": "0.1.0",
  "description": "Chat-first attention visualization for SGLang",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx",
    "test": "playwright test",
    "test:unit": "playwright test --project=unit",
    "test:integration": "RUN_INTEGRATION_E2E=1 playwright test --project=integration",
    "test:all": "RUN_INTEGRATION_E2E=1 playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  }
}
